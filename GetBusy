local mike1 = Instance.new("Animation")
        mike1.AnimationId = "rbxassetid://5099677897"
        local mike2 = Instance.new("Animation")
        mike2.AnimationId = "rbxassetid://5463881468"
        local mike3 = Instance.new("Animation")
        mike3.AnimationId = "rbxassetid://5464124616"
        local loadmike1 = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(mike1)
        local loadmike2 = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(mike2)
        local loadmike3 = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(mike3)
        loadmike1:Play()
        loadmike1:AdjustSpeed()
        wait(0.5)
        loadmike2:Play()
        loadmike2:AdjustSpeed()
        loadmike3:Play()
        loadmike3:AdjustSpeed()
        wait (0.5)
        loadmike3:Stop()
        loadmike2:Stop()
        loadmike1:Stop()
end)

