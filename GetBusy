local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Swrv HUB", "Synapse")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Lounge")

Section:NewKeybind("Reach", "KeybindInfo", Enum.KeyCode.F, function()
	print("HI")
	-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "rebound"
}

game:GetService("ReplicatedStorage").Events.Player.Actions:FireServer(unpack(args))
	
end)

Section:NewKeybind("Force 0 Glitch", "Basically makes you shoot from anywhere depends on yo build", Enum.KeyCode.F, function()
	print("You just clicked the bind")
	function pump()
    local args = {[1] = "Pump"}
    game:GetService("ReplicatedStorage").Events.Player.Actions:FireServer(unpack(args))
end

local args = {
    [1] = "shotType",
    [2] = {
        ["Ac"] = {
