miscSection:NewButton("Drop ball", "Drops the ball", function()
    print("Enabled")
    local args = {
        [1] = "dropPlr"
    }
    game:GetService("ReplicatedStorage").Events.Player.Ball:InvokeServer(unpack(args))
end)


miscSection:NewButton("Force reset player", "Force reset ", function()
    print("Enabled")
    game:GetService("ReplicatedStorage").Events.Player.LoadPlayer:FireServer()
end)

miscSection:NewButton("Infinite stamina ", "Inf stamina..", function()
    print("Enabled")
    local stamina = game:GetService("Players").LocalPlayer.Info.Profile.Stamina
    local staminaChangeCheck = stamina:GetPropertyChangedSignal("Value"):Connect(function()
    stamina.Value =  100000000000
end)
end)





