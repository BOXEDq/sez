local screenToggleMode = 0

fakeSection:NewKeybind("cross animation ", "", Enum.KeyCode.P,function(btn)
            
            local mike1 = Instance.new("Animation")
            mike1.AnimationId = "rbxassetid://5549424092"
            local loadmike1 = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(mike1)
            loadmike1:Play()
            loadmike1:AdjustSpeed()
            wait(0.5)
            loadmike1:Stop()

            local mike1 = Instance.new("Animation")
            mike1.AnimationId = "rbxassetid://4652765004"
            local loadmike1 = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(mike1)
            loadmike1:Play()
            loadmike1:AdjustSpeed()
            wait(0.5)
            loadmike1:Stop()
end)

